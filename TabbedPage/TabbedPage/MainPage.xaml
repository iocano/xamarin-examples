            x:Class="TabbedPage.MainPage"
            xmlns:local="clr-namespace:TabbedPage"
            >

    <ContentPage Title="Page 1" IconImageSource="courses">
        <Label Text="Content 1" HorizontalOptions="Center" VerticalOptions="Center"/>
